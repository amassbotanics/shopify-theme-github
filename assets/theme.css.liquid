.pagination {
  padding: 12px 0;
  clear: both;
  text-align: center;
}

.pagination > li {
  font-family: {{ fonts_font_accent_family }};
  font-style: {{ fonts_font_accent_style }};
  /* font-weight: {{ fonts_font_accent_weight }}; */
  font-weight: 600;
  display: inline-block;
  font-size: 16px;
  text-transform: uppercase;
}

.pagination > li:first-child {
  padding-left: 0;
}

.pagination > li.previous, .pagination > li.next {
  color: {{ settings.text-colour | color_modify: 'alpha', 0.5 }};
  transition: 0.15s cubic-bezier(0.4, 0, 0.2, 1) color;
  padding: 0 20px;
  letter-spacing: 0.09em;
}

.pagination > li.previous:hover, .pagination > li.next:hover {
  color: {{ pagination_hover_light }};
}

.pagination > li.previous a, .pagination > li.next a {
  color: {{ settings.accent-colour }};
  transition: 0.15s cubic-bezier(0.4, 0, 0.2, 1) color;
}

.pagination > li.previous a:hover, .pagination > li.next a:hover {
  color: {{ accent_color_hover }};
}

.pagination > li.position {
  margin: 0 25px;
  color: {{ settings.text-colour }};
  transition: 0.15s cubic-bezier(0.4, 0, 0.2, 1) color;
}

.pagination > li.position:hover {
  color: {{ pagination_hover }};
}

.pagination.jump-to-page .jump-to-link {
  opacity: 0.5;
}

.pagination.jump-to-page .jump-to-link.active {
  opacity: 1;
}

.pagination.jump-to-page .jump-to-link a {
  color: {{ settings.text-colour }};
  transition: 0.15s cubic-bezier(0.4, 0, 0.2, 1) color;
}

.pagination.jump-to-page .jump-to-link a:hover {
  color: {{ pagination_hover }};
}

.pagination.jump-to-page li {
  margin: 0 5px;
  border: 0;
}

.pagination.jump-to-page li:nth-of-type(2):not(.previous) {
  margin-left: 15px;
}

.pagination.jump-to-page li:nth-last-child(2):not(.next) {
  margin-right: 15px;
}

.pagination-arrow__mobile {
  display: none;
}
.pagination-arrow__mobile svg {
  width: 20px;
  height: 20px;
}

@media (max-width: 719px) {
  .pagination-arrow__mobile {
    display: block;
  }
  .pagination-arrow__desktop {
    display: none;
  }
}

